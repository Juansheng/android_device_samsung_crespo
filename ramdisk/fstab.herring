# Android fstab file.
#<src>                                                  <mnt_point>         <type>    <mnt_flags and options>                               <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

/dev/block/mmcblk0p1                                    /system             ext4      ro,barrier=1                                          wait
/dev/block/mmcblk0p2                                    /data               ext4      noatime,nosuid,nodev,barrier=1,nomblk_io_submit       wait,encryptable=/efs/userdata_footer
/devices/platform/s3c-sdhci.0/mmc_host/mmc0/mmc0:0001/block/mmcblk0 /storage/sdcard0    vfat      defaults                                              voldmanaged=sdcard:3,nonremovable
